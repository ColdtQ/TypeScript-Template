{
  "scripts": {
    "build": "npx tsc --build --diagnostics",
    "build-w": "npx tsc --build --diagnostics -w",
    "clean": "npx tsc --build --clean",
    "dev": "cross-env NODE_ENV=development npx ts-node ./src/index.ts",
    "husky:test": "npx tsc --build && rm -r ./dist && npm run lint:husky",
    "lavalink": "java -jar ./Lavalink/Lavalink.jar",
    "lint": "npm run lint:fix && npx run prettier",
    "lint:husky": "npm run lint:check && npm run prettier-check",
    "lint:check": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "prettier": "npx prettier --config .prettierrc src/**/*.ts --write ",
    "prettier-check": "npx prettier --config .prettierrc --check src/**/*.ts",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "start:prod": "cross-env NODE_ENV=production pm2 start dist/index.js --name Template",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve"
  },
  "author": "Cold tQ",
  "license": "",
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "commitlint": "^13.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
