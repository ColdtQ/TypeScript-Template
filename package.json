{
  "devDependencies": {
    "commitlint": "^13.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build": "npx tsc --build --diagnostics",
    "build-w": "npx tsc --build --diagnostics -w",
    "clean": "npx tsc --build --clean",
    "dev": "cross-env NODE_ENV=development npx ts-node ./src/index.ts",
    "dashboard": "cross-env npx next ./src/Struct/Dashboard/app/ -p 1241",
    "husky:test": "npx tsc --build --diagnostics && rm -r ./dist && npm run prettier",
    "lavalink": "java -jar ./Lavalink/Lavalink.jar",
    "lint": "npm run lint:fix && npx run prettier",
    "lint:fix": "npx eslint . --fix",
    "prettier": "npx prettier --config .prettierrc src/**/*.ts --write ",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "start:prod": "cross-env NODE_ENV=production pm2 start dist/index.js --name Fruitical",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve"
  },
  "author": "",
  "license": "ISC"
}
